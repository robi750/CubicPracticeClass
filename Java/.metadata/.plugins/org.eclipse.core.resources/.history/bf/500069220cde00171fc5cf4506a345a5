package com.robi.messengerApp.Service;

import java.util.ArrayList;
import java.util.Date;
import java.util.List;

import com.robi.messengerApp.model.Message;

public class MessageService {

	public List<Message> getAllMessages() {
		Message m1 = new Message(long id, String message, Date created, String outhor));
		Message m2 = new Message(2L, "Hello World", 2017-10-09, "Robin");
		List<Message> list = new ArrayList<>();
		list.add(m1);
		list.add(m2);
		return list;
	}

}
